# Google Sheets API (read-only)
NEXT_PUBLIC_GOOGLE_API_KEY=your-api-key-here

# Google Sheet ID
NEXT_PUBLIC_GOOGLE_SHEET_ID=your-sheet-id-here

# n8n Webhook URLs
# Main feedback submission webhook
NEXT_PUBLIC_N8N_WEBHOOK_URL=https://your-instance.app.n8n.cloud/webhook/your-webhook-id

# Quip creation webhook
NEXT_PUBLIC_N8N_QUIP_WEBHOOK_URL=https://your-instance.app.n8n.cloud/webhook/your-quip-webhook-id
